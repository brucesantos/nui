<!DOCTYPE html>
<html lang="pt-br" dir="ltr">

    <!-- Range color CSS - ini -->
      <style type="text/css">
        #slider12a .slider-track-high, #slider12c .slider-track-high {
          background: green;
        }

        #slider12b .slider-track-low, #slider12c .slider-track-low {
          background: red;
        }

        #slider12c .slider-selection {
          background: yellow;
        }
      </style>
      <!-- Range color CSS - fim -->

      <!-- Slide Range -->
    <link href="dependencies/range/_bootstrap-slider.css" rel="stylesheet">

    <!-- Bootstrap rodando o slider range perfeito
     <link href="dependencies/range/bootstrap.min.css" rel="stylesheet">
   -->



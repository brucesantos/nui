    <!-- Script do Range inicio -->
    <script type="text/javascript" src="dependencies/range/_jquery.min.js"></script>
    <script type="text/javascript" src="dependencies/range/_bootstrap-slider.js"></script>


    <script type="text/javascript">
      $(document).ready(function() {


      /* Example 12 */
      $("#ex12a").slider({
        id: "slider12a",
        min: 0,
        max: 10,
        value: 5
            });
      $("#ex12b").slider({
        id: "slider12b",
        min: 0,
        max: 10,
        range: true,
        value: [ 3, 7 ]
      });
      $("#ex12c").slider({
        id: "slider12c",
        min: 0,
        max: 10,
        range: true,
        value: [ 3, 7 ]
      });


    });
    </script>
    <!-- Script do Range fim -->
  </body>
